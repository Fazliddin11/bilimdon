\n      num:'',\r\n      isActive:false,\r\n      pass:false,\r\n      password:''\r\n    }\r\n  },\r\n  methods:{\r\n    sigIn(){\r\n      let number = this.name\r\n      this.num = number\r\n      axios\r\n        .get(`http://localhost:8080/api/auth/${this.name}`)\r\n        .then(response => {\r\n          // console.log(response.data.code)\r\n          this.code = response.data\r\n          if (response.status === 200){\r\n            this.isActive = !this.isActive\r\n            this.pass = !this.pass\r\n          }\r\n        })\r\n    },\r\n    check(){\r\n      axios\r\n        .get(`http://localhost:8080/api/check/${this.num}&${this.password}`)\r\n        .then(response => {\r\n          console.log(response.data.accessToken)\r\n          if (response.status === 200){\r\n            VueCookies.set('Token' ,response.data.accessToken)\r\n            this.$router.push('/')\r\n          }\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped lang=\"scss\">\r\n  .all{\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    margin: auto;\r\n    .card{\r\n      background-color: white;\r\n      border-radius: 8px;\r\n      border: 0;\r\n      box-shadow: 0 2px 4px rgb(0 0 0 / 10%), 0 8px 16px rgb(0 0 0 / 10%);\r\n      .text{\r\n        text-align: center;\r\n        padding-top: 27px;\r\n        font-size: 26px;\r\n      }\r\n      .inputs{\r\n        display: flex;\r\n        flex-direction: column;\r\n        padding: 30px;\r\n        input{\r\n          border: 1px solid black;\r\n          border-radius: 8px;\r\n          padding-left: 10px;\r\n          margin: 10px;\r\n          height: 47px;\r\n          width: 300px;\r\n        }\r\n      }\r\n      .button{\r\n        button{\r\n          justify-content: center;\r\n          display: flex;\r\n          align-items: center;\r\n          margin: auto;\r\n          background: #0072e3;\r\n          padding: 9px 97px;\r\n          border-radius: 8px;\r\n          margin-bottom: 20px;\r\n          font-size: 23px;\r\n          color: white;\r\n        }\r\n        button{\r\n          &:first-child{\r\n            &.active{\r\n              display: none;\r\n            }\r\n          }\r\n        }\r\n        .check{\r\n          display: none;\r\n          &.active{\r\n            display: block;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .pass--input{\r\n    display: none;\r\n    &.active{\r\n      display: block;\r\n    }\r\n  }\r\n</style>"]}]}